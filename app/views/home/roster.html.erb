<table class="table table-striped">
  <tr>
    <th>
      <strong> Group #</strong>
    </th>
  </tr>
    <% Users.all.each do |user| %>
        <% if user.name != nil %>
            <tr>
                <td> 
                    <%= user.name %> 
                </td>
            </tr>
            <br/>
        <% end %>
    <% end %>

</table>
    
